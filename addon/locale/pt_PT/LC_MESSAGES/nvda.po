msgstr "Lista de separadores"

#. Translators: Nombre para el botón clic izquierdo
#: addon\appModules\chrome.py:378
msgid "Clic en botón &Izquierdo"
msgstr "Pressione em botão esquerdo"

#. Translators: Nombre para el botón clic derecho
#: addon\appModules\chrome.py:382
msgid "Clic en botón &Derecho"
msgstr "Pressione em botão direito"

#. Translators: Nombre para el botón nueva pestaña
#: addon\appModules\chrome.py:386
msgid "&Nueva pestaña"
msgstr "%Novo separador"

#. Translators: Nombre para el botón cerrar
#: addon\appModules\chrome.py:390
msgid "&Cerrar"
msgstr "&Fechar"

